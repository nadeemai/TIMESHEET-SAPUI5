                            </HBox>
                            
                            <Panel id="_IDGenPanel7" headerText="Project Engagement Duration" class="sapUiMediumMarginTop">
                                <table:Table id="_IDGenTable3" rows="{managerView>/projectEngagements}" visibleRowCount="8">
                                    <table:columns>
                                        <table:Column id="_IDGenColumn34">
                                            <Label id="_IDGenLabel50" text="Project"/>
                                            <table:template>
                                                <Text id="_IDGenText43" text="{managerView>projectName}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn35">
                                            <Label id="_IDGenLabel51" text="Employee"/>
                                            <table:template>
                                                <Text id="_IDGenText44" text="{managerView>employeeName}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn36">
                                            <Label id="_IDGenLabel52" text="Start Date"/>
                                            <table:template>
                                                <Text id="_IDGenText45" text="{path: 'managerView>startDate', type: 'sap.ui.model.odata.type.Date'}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn37">
                                            <Label id="_IDGenLabel53" text="End Date"/>
                                            <table:template>
                                                <Text id="_IDGenText46" text="{path: 'managerView>endDate', type: 'sap.ui.model.odata.type.Date'}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn38">
                                            <Label id="_IDGenLabel54" text="Duration (Days)"/>
                                            <table:template>
                                                <Text id="_IDGenText47" text="{managerView>duration}"/>
                                            </table:template>
                                        </table:Column>
                                    </table:columns>
                                </table:Table>
                            </Panel>
                            
                            <Panel id="_IDGenPanel8" headerText="Booked Hours Analysis" class="sapUiMediumMarginTop">
                                <table:Table id="_IDGenTable4" rows="{managerView>/bookedHours}" visibleRowCount="8">
                                    <table:columns>
                                        <table:Column id="_IDGenColumn39">
                                            <Label id="_IDGenLabel55" text="Project"/>
                                            <table:template>
                                                <Text id="_IDGenText48" text="{managerView>projectName}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn40">
                                            <Label id="_IDGenLabel56" text="Employee"/>
                                            <table:template>
                                                <Text id="_IDGenText49" text="{managerView>employeeName}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn41">
                                            <Label id="_IDGenLabel57" text="Planned Hours"/>
                                            <table:template>
                                                <Text id="_IDGenText50" text="{managerView>plannedHours}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn42">
                                            <Label id="_IDGenLabel58" text="Actual Hours"/>
                                            <table:template>
                                                <Text id="_IDGenText51" text="{managerView>actualHours}"/>
                                            </table:template>
                                        </table:Column>
                                        <table:Column id="_IDGenColumn43">
                                            <Label id="_IDGenLabel59" text="Variance"/>
