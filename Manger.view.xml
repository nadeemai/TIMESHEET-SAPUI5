                            </table:template>
                        </table:Column>
                    </table:columns>
                </table:Table>
            </Panel>
        </Panel>
    </VBox>
</IconTabFilter>
                    
                    <!-- Manager's View Tab -->
                    <IconTabFilter id="_IDGenIconTabFilter4" text="Manager's View" icon="sap-icon://manager">
                        <VBox id="_IDGenVBox8" class="sapUiMediumMargin">
                            <HBox id="_IDGenHBox11">
                                <Select id="managerProjectSelect" items="{/projects}" change="onManagerViewChange" class="sapUiMediumMarginEnd">
