                            <table:Table id="employeeTable" rows="{/employees}" selectionMode="MultiToggle" visibleRowCount="10">
                                <table:columns>
                                    <table:Column id="_IDGenColumn">
                                        <Label id="_IDGenLabel32" text="Employee"/>
                                        <table:template>
                                            <Text id="_IDGenText25" text="{firstName} {lastName}"/>
                                        </table:template>
                                    </table:Column>
                                    <table:Column id="_IDGenColumn17">
                                        <Label id="_IDGenLabel33" text="Role"/>
                                        <table:template>
                                            <Text id="_IDGenText26" text="{roleName}"/>
                                        </table:template>
                                    </table:Column>
                                    <table:Column id="_IDGenColumn18">
                                        <Label id="_IDGenLabel34" text="Email"/>
                                        <table:template>
                                            <Text id="_IDGenText27" text="{email}"/>
                                        </table:template>
                                    </table:Column>
                                    <table:Column id="_IDGenColumn20">
                                        <Label id="_IDGenLabel36" text="Manager"/>
                                        <table:template>
                                            <Text id="_IDGenText28" text="{managerName}"/>
                                        </table:template>
                                    </table:Column>
                                </table:columns>
                            </table:Table>
                            <HBox id="_IDGenHBox5" class="sapUiMediumMarginTop">
                                <Select id="projectSelect" items="{/projects}" class="sapUiMediumMarginEnd">
                                    <items>
                                        <core:Item id="_IDGenItem24" key="{id}" text="{name}"/>
                                    </items>
