                                            </table:template>
                                        </table:Column>
                                    </table:columns>
                                </table:Table>
                            </Panel>
                            
                            <Panel id="_IDGenPanel9" headerText="Potential Issues" class="sapUiMediumMarginTop">
                                <List id="_IDGenList2" items="{managerView>/potentialIssues}">
                                    <StandardListItem id="_IDGenStandardListItem1" 
                                        title="{title}" 
                                        description="{description}"
                                        icon="{icon}"
                                        type="Active"/>
                                </List>
                            </Panel>
                        </VBox>
                    </IconTabFilter>
                </items>
            </IconTabBar>
        </content>
    </Page>
